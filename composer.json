{
  "name": "tekord/laravel-logman",
  "description": "Adds manageable container of context data for logs.",
  "keywords": ["laravel", "log", "logging", "logger"],
  "license": "MIT",
  "authors": [
    {
      "name": "Cyrill Tekord",
      "email": "tekord@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "5.5.*|5.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "6.0.*|7.0.*",
    "orchestra/testbench": "3.5.*|3.6.*"
  },
  "autoload": {
    "psr-4": {
      "Tekord\\Logman\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tekord\\Logman\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "aliases": {
        "Logman": "Tekord\\Logman\\Facades\\Logman"
      },
      "providers": [
        "Tekord\\Logman\\ServiceProvider"
      ]
    }
  }
}
